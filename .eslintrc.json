{
  "extends": ["next", "next/core-web-vitals", "prettier"],
  "rules": {
    "no-console": "warn",
    "no-mixed-spaces-and-tabs": "error",
    "no-unused-vars": "warn",
    "no-unused-expressions": "warn",
    "import/order": [
      "error",
      {
        "alphabetize": {
          "order": "asc"
        }
      }
    ]
  }
}
